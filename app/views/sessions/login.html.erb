<h3>Log In</h3>
<div>
<%= form_with do |f| %>

    <%= f.label :email %>
    <%= f.email_field(:email) %><br>

    <%= f.label :password %>
    <%= f.password_field(:password) %><br>

    <%= f.submit "Log In" %>

    <% end %>
</div>

    <%= link_to 'Log In with Github', '/auth/github/callback', method: :post %>